@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

@foreach (var post in @Model.Posts)
{
    <div class="jumbotron jumbotron-fluid">
        <div class="container">
            <h2 class="text-center">@post.Title</h2>
            <p class="mt-6">@post.Content</p>
        </div>
    </div>
}